{
    "manifest_version": 2,
    "name": "Full Page Debugger",
    "version": "0.1",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["jquery-2.2.2.min.js", "content.js"]
    }],
    "background": {
        "scripts": ["background.js"],
        "persistant": true
    },
    "browser_action": {
        "default_icon": "off.ico",
        "default_title": "Debug: OFF"
    }
}
